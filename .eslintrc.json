{
  "env": {
    "mocha": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "root": true,
  "rules": {
    "arrow-body-style": ["error", "always"],
    "comma-dangle": ["error", "never"],
    "semi": ["error", "never"]
  }
}
